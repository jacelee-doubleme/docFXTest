{
  "metadata": [
    {
      "src": [
        {
          "src": "../Assets",
          "files": [
            "**.cs"
          ],
          "exclude": [

          ],
		  "globalNamespaceId": "Global",
            "filter": "filterConfig.yml",
            "dest": "api"
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "toc.yml",
          "index.md"
        ]
      },
      {
        "src": "api",
        "files": [
          "*.yml"
        ],
        "dest": "api"
      },
      {
        "src": "manual",
        "files": [
          "toc.yml",
          "*.md"
        ],
        "dest": "manual"
      }
    ],
	"resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "globalMetadata": {
      "_appName": "DocFXTest",
      "_appTitle": "DocFXTest",
	  "_appLogoPath": "images/logo.png",
      "_enableSearch": true
    },
    "dest": "_site"
  }
}