{
  "$schema": "https://raw.githubusercontent.com/dotnet/docfx/main/schemas/docfx.schema.json",
  "metadata": [
    {
      "src": [
        {
          "src": "../Assets",
          "files": [
            "**.cs"
          ]
        }
      ],
      "dest": "api",
	  "properties": {
        "TargetFramework": "net471",
        "TargetFrameworkIdentifier": ".NETFramework",
        "TargetFrameworkVersion": "v4.7.1",
        "UseSharedCompilation": "false",
        "MSBuildSDKsPath": "",
        "MSBuildExtensionsPath": "C:/Program Files (x86)/Microsoft Visual Studio/2019/Enterprise/MSBuild",
        "FrameworkPathOverride": "C:/Program Files (x86)/Reference Assemblies/Microsoft/Framework/.NETFramework/v4.7.1"
      },
      "references": [
  "C:/Program Files/Unity/Hub/Editor/2022.3.54f1/Editor/Data/Managed/UnityEngine.dll",
  "C:/Program Files/Unity/Hub/Editor/2022.3.54f1/Editor/Data/Managed/UnityEngine.CoreModule.dll"
]
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
        ],
        "exclude": [
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "output": "_site",
    "template": [
      "default",
      "modern"
    ],
    "globalMetadata": {
      "_appName": "DocFXTest",
      "_appTitle": "DocFXTest",
      "_enableSearch": true,
      "pdf": true
    }
  }
}